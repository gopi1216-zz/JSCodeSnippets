<script>
setInterval(function(){
  var ele = document.getElementById('demo'),
      text = ele.innerHTML,
      str = text;
  str = str[str.length - 1] + str.substring(0, str.length - 1);
  ele.innerHTML = str;
  return console.log(ele.innerHTML);
}, 1000);

</script>
